#include&lt;stdio.h&gt;
void main()
{
int num1, num2;
printf(“Enter any two integers:”);
scanf(“%d %d”, &amp;num1, &amp;num2);
printf(“Before Interchange: num1=%d, num2=%d \n”, num1, num2);
num1=num1+num2;
num2= num1-num2;
num1=num1-num2;
printf(“After interchange: num1=%d, num2=%d” , num1, num2);
}
